var _objectKeys = function(obj) {
if (obj == null) {
return [];
}
if (Object.keys) {
return Object.keys(obj);
}
var keys = [];
for (var prop in obj) {
if (obj.hasOwnProperty(prop)) {
keys.push(prop);
}
}
return keys;
};
var _deleteOwnProperties = function(obj) {
if (obj) {
for (var prop in obj) {
if (obj.hasOwnProperty(prop)) {
delete obj[prop];
}
}
}
return obj;
