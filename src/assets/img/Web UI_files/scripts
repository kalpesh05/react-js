ZeroClipboard.prototype.unclip = function(elements) {
var meta = _clientMeta[this.id];
if (meta) {
var clippedElements = meta.elements;
var arrayIndex;
if (typeof elements === "undefined") {
elements = clippedElements.slice(0);
} else {
elements = _prepClip(elements);
}
